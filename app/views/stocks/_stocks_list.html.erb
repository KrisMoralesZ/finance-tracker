<div>
  <table class="table table-striped table-condensted">
    <thead>
    <tr>
      <th>Symbol</th>
      <th>Name</th>
    </tr>
    </thead>
    <tbody id="stocksTable">
    <% @stocks.each do |stock|%>
      <tr>
        <td><%= link_to stock.ticker, stock_path(stock.ticker) %></td>
        <td><%= stock.security %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
